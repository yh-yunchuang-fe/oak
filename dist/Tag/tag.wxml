<wxs src="../wxs/base.wxs" module="Utils" />

<view class="{{ Utils.classNames('oak-tag', {plain: plain})}} oak-tag-{{color}} oak-tag-{{shape}}  custom-class" style="{{ _style }}">
    <image wx:if="{{beforeImage}}" mode="aspectFill" class="image" src="{{beforeImage}}" />
    <slot></slot>
    <image wx:if="{{afterImage}}" mode="aspectFill" class="image" src="{{afterImage}}" />
</view>
