.inline-block {
  display: inline-block;
  vertical-align: middle;
}
.tc {
  text-align: center;
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.75);
}
.oak-tag {
  display: inline-flex;
  justify-content: center;
  align-items: center;
  width: max-content;
  min-width: 40rpx;
  padding: 0 4rpx;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 20rpx;
  border-radius: 4rpx;
  text-align: center;
  box-sizing: content-box;
  vertical-align: middle;
}
.oak-tag-default {
  color: #333;
  background-color: #fafafa;
  border: 2rpx solid #d9d9d9;
  font-family: PingFangSC-Regular, PingFang SC;
}
