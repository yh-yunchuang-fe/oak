.oak-tag {
  min-width: 40rpx;
  padding: 0 4rpx;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 20rpx;
  border-radius: 4rpx;
  display: inline-block;
  text-align: center;
  box-sizing: content-box;
  vertical-align: top; }
