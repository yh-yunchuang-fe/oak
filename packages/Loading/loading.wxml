<wxs module ="GlobalLoading">
    var crab = 'data:image/png;base64,'
    var orange = 'data:image/png;base64,'
    var cabbage = 'data:image/png;base64,'
    module.exports = {
        crab: crab,
        orange: orange,
        cabbage: cabbage
    }
</wxs>

<view class="oak-loading {{size}} {{position}} ext-class" wx:if="{{isShow && type === 'default'}}">
    <view wx:if="{{shadow}}" class="item shadow"/>
    <view wx:for="{{[1,2,3]}}" wx:key="index" class='item loader loader-class' style="border-top-color: {{color}};"></view>
</view>

<view class="oak-global-loading" wx:if="{{type === 'global'}}">
    <view class="loading-mask" hidden="{{!mask}}" />
    <view class="loading-wrapper" hidden="{{!isShow}}" > 
        <image mode="aspectFit"  class="img-item img-item-crab" src="{{GlobalLoading.crab}}"/>
        <image mode="aspectFit"  class="img-item img-item-cabbage" src="{{GlobalLoading.cabbage}}"/>
        <image mode="aspectFit"  class="img-item img-item-orange" src="{{GlobalLoading.orange}}"/>
        <view class="shadow"/>
    </view>
</view>
