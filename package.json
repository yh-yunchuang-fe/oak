{
    "name": "@yhyc/oak-weui",
    "version": "1.0.16",
    "description": "A Wechat App UI Lib.",
    "miniprogram": "dist",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "./node_modules/.bin/eslint 'src/**/*.ts'",
        "start": "cross-env NODE_ENV=development gulp --gulpfile __build__/gulpfile.babel.js",
        "packages": "cross-env NODE_ENV=development gulp packages --gulpfile __build__/gulpfile.babel.js",
        "build": "cross-env NODE_ENV=production gulp --gulpfile __build__/gulpfile.babel.js && rm -rf dist/style && npm run release &&  npm run packages",
        "new": "node __build__/create",
        "qrcode": "node __build__/weapp_qrcode",
        "docs": "cross-env NODE_ENV=production webpack --color --config __build__/webpack.site.prod.js && gh-pages -d docs/dist",
        "docs:dev": "webpack-dev-server --config __build__/webpack.site.dev.js",
        "release": "tar -zcf oak.tar.gz dist/* && zip -r oak.zip dist/*",
        "upload_static": "gulp --gulpfile __build__/gulpfile.babel.js uploadImageMini && gulp --gulpfile __build__/gulpfile.babel.js upload2Qiniu"
    },
    "husky": {
        "hooks": {
            "pre-commit": "./__build__/pre-commit.sh",
            "pre-push": "./__build__/pre-commit.sh"
        }
    },
    "files": [
        "dist",
        "packages"
    ],
    "repository": {
        "type": "git",
        "url": "git+git@github.com:yh-yunchuang-fe/oak.git"
    },
    "keywords": [
        "wechat",
        "weapp",
        "ui"
    ],
    "author": "YHFE",
    "email": "oakui@outlook.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/yh-yunchuang-fe/oak/issues"
    },
    "homepage": "https://yh-yunchuang-fe.github.io/oak",
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-syntax-class-properties": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@babel/register": "^7.6.2",
        "@loadable/component": "^5.10.3",
        "@typescript-eslint/eslint-plugin": "^1.13.0",
        "@typescript-eslint/parser": "^1.13.0",
        "antd": "^3.24.3",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-import": "^1.12.2",
        "babel-preset-react-hmre": "^1.1.1",
        "clean-webpack-plugin": "^2.0.2",
        "core-js": "^2.6.10",
        "cross-env": "^5.2.1",
        "css-loader": "^2.1.1",
        "del": "^4.1.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-react": "^7.16.0",
        "eslint-plugin-typescript": "^0.14.0",
        "express": "^4.17.1",
        "fs-extra": "^7.0.1",
        "gh-pages": "^2.1.1",
        "gulp": "^4.0.2",
        "gulp-changed": "^3.2.0",
        "gulp-csso": "^3.0.1",
        "gulp-eslint": "^5.0.0",
        "gulp-if": "^2.0.2",
        "gulp-imagemin": "^6.1.1",
        "gulp-less": "^4.0.1",
        "gulp-print": "^5.0.0",
        "gulp-qiniu-utils": "2.0.1",
        "gulp-rename": "^1.4.0",
        "gulp-typescript": "^5.0.1",
        "gulp-uglify-es": "^1.0.4",
        "gulp-util": "^3.0.8",
        "gulp-wechat-weapp-src-alisa": "^1.0.2",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^3.0.9",
        "less-loader": "^5.0.0",
        "mini-css-extract-plugin": "^0.6.0",
        "miniprogram-api-typings": "^2.9.0",
        "node-fetch": "^2.6.0",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^2.0.0",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "react-markdown": "^4.2.2",
        "react-router-dom": "^5.1.2",
        "react-scroll": "^1.7.14",
        "react-syntax-highlighter": "^10.3.5",
        "sass-loader": "^7.3.1",
        "style-loader": "^0.23.1",
        "typescript": "^3.8.3",
        "upload-to-qiniu-webpack-plugin": "^1.0.3",
        "webpack": "^4.41.2",
        "webpack-ant-icon-loader": "^1.0.8",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.9",
