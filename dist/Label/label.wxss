.oak-label {
  min-width: 24rpx;
  height: 56rpx;
  line-height: 56rpx;
  display: inline-flex;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #666;
  font-size: 24rpx;
  border: 1rpx solid #F6F6F6;
  border-radius: 200rpx;
  background-color: #F6F6F6;
}
.oak-label::before {
  font-size: 24rpx;
  margin-right: 10rpx;
}
.oak-label-normal {
  max-width: calc(100% - 48rpx);
  padding: 0 24rpx;
}
.oak-label-checked {
  font-size: 24rpx;
  color: #FD7622;
  border: 1rpx solid #FD7622;
  background-color: rgba(253, 118, 34, 0.08);
}
.oak-label-checked::after {
  content: "";
  width: 1rpx;
  height: 100%;
  float: right;
}
.oak-label-content {
  max-width: 100%;
  margin: 0 auto;
  display: inline-block;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
