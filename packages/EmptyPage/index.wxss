.empty {
    width: 100%;
    display: flex;
    align-items: center;
    flex-direction: column;    
}

.empty.fixed,
.empty.absolute {
    left: 50%;
    top: 50%;
    transform: translate(-50%, -60%);
    z-index: 14;
}
.empty .icon {
    width: 256rpx;
}
.empty .desc {
    margin-top: 30rpx;
    font-size: 14px;
    color: #999999;
    text-align: center;
}
.empty.fixed,.empty-pro {
    top: 60%;
}
