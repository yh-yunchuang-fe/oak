.inline-block {
  display: inline-block;
  vertical-align: middle;
}
.tc {
  text-align: center;
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.75);
}
.oak-label {
  max-width: 100%;
  min-width: 24rpx;
  height: 56rpx;
  display: inline-flex;
  color: #666;
  font-size: 24rpx;
  border: 1rpx solid #F6F6F6;
  border-radius: 200rpx;
  background-color: #F6F6F6;
  align-items: center;
  justify-content: center;
  line-height: 1;
  box-sizing: border-box;
}
.oak-label .oak-icon:first-child {
  margin-right: 10rpx;
}
.oak-label .oak-icon:last-child {
  margin-left: 10rpx;
}
.oak-label .oak-icon-ext {
  color: inherit;
  font-size: inherit;
}
.oak-label-normal {
  padding: 0 24rpx;
}
.oak-label-checked {
  font-size: 24rpx;
  color: #FD7622;
  border: 1rpx solid #FD7622;
  background-color: rgba(253, 118, 34, 0.08);
}
.oak-label-checked::after {
  content: "";
  width: 1rpx;
  height: 100%;
  float: right;
}
.oak-label-content {
  flex: 1;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  height: auto;
}
